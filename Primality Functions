number = int(input('what is your lucky number? '))
dividors = range(1, number + 1)

def get_prime_number(number):
    for dividor in dividors:
        if number == 0 or 1:
            return 'try again'
        elif number % dividor == 0:
            return "you chose prime number"
        else:
            return f"{number} is not prime number"

print(get_prime_number(number))
